{
  "name": "test-esy-python",
  "version": "0.0.0",
  "description": "Python  (CPython) packaged for esy",
  "license": "MIT",
  "scripts": {
    "example": "python3 hello.py"
  },
  "dependencies": {
    "python": "*"
  },
  "resolutions": {
    "python": {
      "source": "https://www.python.org/ftp/python/3.7.1/Python-3.7.1.tgz#md5:99f78ecbfc766ea449c4d9e7eda19e83",
      "override": {
        "buildsInSource": true,
        "build": [
          "./configure --prefix #{self.install}",
          "make"
        ],
        "install": [
          "make install"
        ]
      }
    }
  }
}
